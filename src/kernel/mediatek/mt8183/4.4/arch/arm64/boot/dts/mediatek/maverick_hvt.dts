&i2c6 {
	rt551x: audio-codec@57 {
		compatible = "realtek,rt551x";
		#sound-dai-cells = <0>;
		reg = <0x57>;
		interrupt-parent = <&pio>;
		interrupts = <169 IRQ_TYPE_LEVEL_HIGH 169 0>;
		rt5518-reset = <&pio 178 0>;
		hw_analog_gain = <0xe>;
		hw_digital_gain = <0x8927>;
		pinctrl-names = "default", "rt551x_hwreset_enable",
				"rt551x_hwreset_disable";
		pinctrl-0 = <&rt551x_default>;
		pinctrl-1 = <&rt551x_hwreset_enable>;
		pinctrl-2 = <&rt551x_hwreset_disable>;
	};
};

